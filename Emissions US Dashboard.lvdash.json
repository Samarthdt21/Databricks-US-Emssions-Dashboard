{
  "datasets": [
    {
      "name": "0c73046f",
      "displayName": "Location Data",
      "queryLines": [
        "select county_state_name,latitude,longitude,`GHG emissions mtons CO2e` as emissions from emissions_data"
      ],
      "catalog": "emissions_us",
      "schema": "default"
    },
    {
      "name": "0b2b7353",
      "displayName": "Emission per person",
      "queryLines": [
        "select \r\n",
        "       county_state_name,\r\n",
        "       population,\r\n",
        "       cast(\r\n",
        "         replace(`GHG emissions mtons CO2e`, ',', '') as double\r\n",
        "       ) / cast(\r\n",
        "         replace(population, ',', '') as double\r\n",
        "       ) as emissions_per_person\r\n",
        "from emissions_data\r\n",
        "order by emissions_per_person desc\r\n",
        "\r\n"
      ],
      "catalog": "emissions_us",
      "schema": "default"
    },
    {
      "name": "af5c4d48",
      "displayName": "Total emission by state",
      "queryLines": [
        "select \r\n",
        "       state_abbr,\r\n",
        "       sum(cast(\r\n",
        "         replace(`GHG emissions mtons CO2e`, ',', '') as double\r\n",
        "       )) as Total_emissions\r\n",
        "from emissions_data\r\n",
        "group by state_abbr     \r\n",
        "order by Total_emissions desc\r\n",
        "\r\n",
        "\r\n"
      ],
      "catalog": "emissions_us",
      "schema": "default"
    },
    {
      "name": "0d709799",
      "displayName": "Emissions by county",
      "queryLines": [
        "select \r\n",
        "       county_state_name,\r\n",
        "       population,\r\n",
        "       cast(\r\n",
        "         replace(`GHG emissions mtons CO2e`, ',', '') as double\r\n",
        "       ) as total_emissions\r\n",
        "from emissions_data\r\n",
        "order by total_emissions desc\r\n",
        "limit 10\r\n",
        "\r\n"
      ],
      "catalog": "emissions_us",
      "schema": "default"
    }
  ],
